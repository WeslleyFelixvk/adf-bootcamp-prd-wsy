						"type": "string"
					},
					"p_data_inicio_carga": {
						"type": "string"
					}
				},
				"variables": {
					"v_data_particao": {
						"type": "String"
					},
					"v_diretorio_destino": {
						"type": "String"
					},
					"v_arquivo_destino": {
						"type": "String"
					},
					"v_diretorio_completo": {
						"type": "String"
					},
					"v_rascunho_nome_tabela_delta": {
