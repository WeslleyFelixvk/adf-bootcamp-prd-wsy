					},
					"v_etapa_bronze": {
						"type": "Boolean"
					}
				},
				"folder": {
					"name": "2.3 Raw e Bronze/Web API/Dummy JS"
				},
				"annotations": [
					"@pipeline().parameters.p_colecao_origem"
				],
				"lastPublishTime": "2022-05-11T11:13:04Z"
			},
			"dependsOn": []
		},
